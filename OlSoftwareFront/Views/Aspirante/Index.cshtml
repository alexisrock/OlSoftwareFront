@model List<OlSoftwareFront.Model.AspirantesResponse>


<div class="d-flex flex-wrap flex-row">

	<div class="table-responsive">

		<table class="table table-bordered table-striped" id="tblproductos">
			<thead>
				<tr>
					<th>Nombres</th>
					<th>Docuemento</th>
					<th>Telefono</th>
					<th>Correo</th>
					<th>Accion</th>
				</tr>

			</thead>
			<tbody>

				@foreach (var usuario in Model)
				{
					<tr>
						<td>@usuario.nombres</td>
						<td>@usuario.numDocumento</td>
						<td> @usuario.telefono</td>
						<td> @usuario.email</td>

						</tr>
				}
			</tbody>

		</table>

	</div>

</div>
